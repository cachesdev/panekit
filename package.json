{
	"name": "panekit",
	"description": "headless window manager toolkit for Svelte 5",
	"version": "0.6.0",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"url": "https://github.com/cachesdev/panekit"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && npx @tailwindcss/cli -i index.css -o ../../dist/index.css --minify --cwd ./src/lib/ && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"changeset": "changeset",
		"changeset:version": "changeset version && npm install --package-lock-only",
		"changeset:publish": "changeset publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!dist/assets/**",
		"!dist/index.css",
		"src/lib",
		"!src/lib/index.css",
		"!src/lib/assets/**",
		"!src/lib/**/*.test.*",
		"!src/lib/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/cli": "^4.1.13",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"keywords": [
		"svelte",
		"windows",
		"panes",
		"window manager",
		"stacking",
		"svelte5",
		"resizeable",
		"draggable"
	],
	"dependencies": {
		"@neodrag/svelte": "^3.0.0-next.8",
		"clsx": "^2.1.1",
		"runed": "^0.31.1",
		"tailwind-merge": "^3.3.1"
	}
}
